{
  "name": "@resistdesign/voltra",
  "version": "0.0.0",
  "description": "With our powers combined!",
  "homepage": "https://voltra.app",
  "repository": "git@github.com:resistdesign/voltra.git",
  "author": "Resist Design",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "vest": "common/Testing/CLI.js"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./api": {
      "types": "./api/index.d.ts",
      "import": "./api/index.js"
    },
    "./app": {
      "types": "./app/index.d.ts",
      "import": "./app/index.js"
    },
    "./build": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    "./common": {
      "types": "./common/index.d.ts",
      "import": "./common/index.js"
    },
    "./iac": {
      "types": "./iac/index.d.ts",
      "import": "./iac/index.js"
    },
    "./iac/packs": {
      "types": "./iac/packs/index.d.ts",
      "import": "./iac/packs/index.js"
    }
  },
  "files": [
    "*.d.ts",
    "*.d.ts.map",
    "index.js",
    "api/**",
    "app/**",
    "build/**",
    "common/**",
    "iac/**",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsup && yarn prep-dist",
    "test": "tsx src/common/Testing/CLI.ts \"./src/**/*.spec.json\"",
    "test:gen": "tsx src/common/Testing/CLI.ts --generate \"./src/**/*.spec.json\"",
    "test:consumer": "node ./scripts/consumer-smoke.mjs",
    "doc": "typedoc",
    "doc:dev": "typedoc --watch",
    "doc-to-site": "cp -r ./docs site-dist/app/",
    "prep-dist": "node ./scripts/prep-dist.mjs",
    "site:build:app": "astro build && yarn finalize-site",
    "site:build:api": "node ./site/build-api.mjs",
    "site:build:iac": "tsx ./site/iac/index.ts",
    "site:build:demo-types": "tsx ./site/build-demo-types.ts",
    "start": "astro dev --host docs-local.demo.voltra.app --https",
    "finalize-site": "yarn doc && yarn doc-to-site && cp CNAME site-dist/app/",
    "iac:types:gen": "tsx ./src/iac/types/generate.ts",
    "src:build:api-orm-driver-config-types": "tsx ./scripts/prebuild-api-orm-driver-config-types.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@aws-sdk/util-dynamodb": "^3.490.0",
    "@fontsource/montserrat-subrayada": "^5.2.5",
    "@picocss/pico": "^2.1.1",
    "ace-builds": "^1.32.3",
    "fast-glob": "^3.3.3",
    "picocolors": "^1.1.1",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.8",
    "typescript": "^5.3.3",
    "uuid": "^11.0.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@astrojs/react": "^4.3.1",
    "@types/cfn-response": "^1.0.8",
    "@types/node": "^20.11.6",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "astro": "^5.13.5",
    "buffer": "^5.5.0||^6.0.0",
    "cfn-response": "^1.0.1",
    "esbuild": "^0.25.9",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "svgo": "^3",
    "ts-morph": "^24.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.15",
    "typedoc-material-theme": "^1.4.1"
  }
}
