{
  "name": "@resistdesign/voltra",
  "version": "0.0.0",
  "description": "With our powers combined!",
  "main": "dist/voltra.cjs.js",
  "module": "dist/voltra.esm.js",
  "browser": "dist/voltra.browser.js",
  "types": "dist/types/index.d.ts",
  "repository": "git@github.com:resistdesign/voltra.git",
  "author": "Resist Design",
  "license": "UNLICENSED",
  "private": true,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@resistdesign"
  },
  "scripts": {
    "build": "parcel build src/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@aws-sdk/util-dynamodb": "^3.490.0",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.7",
    "@types/styled-components": "^5.1.34",
    "parcel": "^2.11.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "targets": {
    "main": {
      "context": "node",
      "includeNodeModules": false,
      "outputFormat": "commonjs",
      "distDir": "dist/node"
    },
    "browser": {
      "context": "browser",
      "includeNodeModules": false,
      "outputFormat": "esmodule",
      "distDir": "dist/browser"
    }
  },
  "parcel": {
    "extends": [
      "@parcel/config-default"
    ],
    "transformers": {
      "*.{js,jsx,ts,tsx}": [
        "@parcel/transformer-typescript",
        "@parcel/transformer-js"
      ]
    }
  }
}
