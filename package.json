{
  "name": "@resistdesign/voltra",
  "version": "0.0.0",
  "description": "With our powers combined!",
  "homepage": "https://voltra.app",
  "repository": "git@github.com:resistdesign/voltra.git",
  "author": "Resist Design",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "voltra-test": "./src/common/TestUtils.ts"
  },
  "scripts": {
    "build": "tsc && yarn prep-dist",
    "test": "tsc --noEmit",
    "doc": "typedoc",
    "doc:dev": "typedoc --watch",
    "doc-to-site": "cp -r ./docs site-dist/app/",
    "prep-dist": "cp package.json dist/ && cp README.md dist/ && cp .releaserc dist/",
    "site:build:app": "parcel build site/app/index.html --dist-dir site-dist --target app && yarn finalize-site",
    "site:build:api": "parcel build ./site/api/index.ts --dist-dir site-dist --target api --no-source-maps",
    "site:build:iac": "ts-node ./site/iac/index.ts",
    "start": "parcel site/app/index.html --dist-dir site-dist/app --https",
    "finalize-site": "yarn doc && yarn doc-to-site && cp CNAME site-dist/app/ && cp -r ./images site-dist/app/ && cp ./favicon/assets/* site-dist/app/",
    "iac:types:gen": "ts-node ./src/iac/types/generate.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@aws-sdk/util-dynamodb": "^3.490.0",
    "ace-builds": "^1.32.3",
    "material-symbols": "^0.14.6",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "styled-components": "^6.1.8",
    "typescript": "^5.3.3",
    "uuid": "^11.0.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-types": "2.11.0",
    "@picocss/pico": "^2.0.6",
    "@types/cfn-response": "^1.0.8",
    "@types/node": "^20.11.6",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1545.0",
    "buffer": "^5.5.0||^6.0.0",
    "cfn-response": "^1.0.1",
    "parcel": "^2.12.0",
    "prettier": "^3.1.1",
    "process": "^0.11.10",
    "react-dom": "^18.3.1",
    "semantic-release": "^23.0.0",
    "ts-morph": "^24.0.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.8",
    "typedoc-material-theme": "^1.0.2"
  },
  "targets": {
    "app": {
      "context": "browser",
      "engines": {
        "browsers": [
          "last 2 version",
          "not dead",
          "> 0.2%"
        ]
      },
      "includeNodeModules": true,
      "outputFormat": "commonjs",
      "isLibrary": false,
      "scopeHoist": false
    },
    "api": {
      "context": "node",
      "engines": {
        "node": ">=10"
      },
      "sourceMap": false,
      "includeNodeModules": true,
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  }
}
